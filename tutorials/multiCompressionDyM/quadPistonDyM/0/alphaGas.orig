FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphaGas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

injection	 	1;

boundaryField
{
    // Injection
	"injectionXMinus|injectionXPlus|injectionYMinus|injectionYPlus"
    {
        type            fixedValue;
        value           uniform $injection;
    }

	// Inlet
	"inletXMinus|inletXPlus|inletYMinus|inletYPlus"
    {
        type            zeroGradient;
    }

	// Outlet
    outlet
    {
		type            zeroGradient;
    }
	
	// Walls
    "piston|valve|walls"
    {
        type            slip;
    }
}

