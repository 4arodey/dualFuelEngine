FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions			[1 -3 0 0 0 0 0]; // kg/m^3

internalField   	uniform 2.735; // 11e05/287.3/1400 = 2.73482173934662622445

boundaryField
{
	// Walls
    "movingWall|walls"
    {
        type        zeroGradient;
    }
}

