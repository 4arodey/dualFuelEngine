FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        // type            uniformFixedValue;
        // uniformValue    constant (0 180 0);
        // uniformValue    table
        // (
        //     (0          (0 180 0)) /* +U    •--•       */
        //     (0.0005     (0 200 0)) /*  |   /    \      */
        //     (0.0010     (0 200 0)) /*  |  •      \     */
        //     (0.0015     (0 50  0)) /* 50          •--• */
        // );

        type            flowRateInletVelocity;
        massFlowRate    0.3;
        // massFlowRate    table
        // (
        //     (0          0.29) /* phi     •--•       */
        //     (0.0005     0.31) /*  |     /    \      */
        //     (0.0010     0.31) /*  |    •      \     */
        //     (0.0015     0.07) /* 0.07          •--• */
        // );
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    walls
    {
        type            slip;
    }
}

