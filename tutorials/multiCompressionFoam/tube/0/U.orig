FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        // uniformValue    constant (0 400 0);
        uniformValue    table
        (
            (0          (0 100 0)) /* +U    •--•       */
            (0.0005     (0 400 0)) /*  |   /    \      */
            (0.0010     (0 400 0)) /*  |  •      \     */
            (0.0015     (0 50  0)) /* 50          •--• */
        );
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    walls
    {
        type            slip;
    }
}

// ************************************************************************* //