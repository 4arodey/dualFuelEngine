FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table // constant (0 400 0);
        (
            (0          (0 50  0)) /* +U   •-•        */
            (2.5e-04    (0 400 0)) /*  |  /   \       */
            (3.0e-04    (0 400 0)) /*  | •     \      */
            (3.5e-04    (0 25  0)) /* 25        •---• */
        );
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    walls
    {
        type            slip;
    }
}

// ************************************************************************* //