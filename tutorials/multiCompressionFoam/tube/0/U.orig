FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    constant (0 400 0);
        // uniformValue    table
        // (
        //     (0          (0 0   0))
        //     (2e-03      (0 400 0))
        //     (5e-03      (0 400 0))
        //     (9e-03      (0 25  0))
        // );
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    walls
    {
        type            slip;
    }
}

// ************************************************************************* //