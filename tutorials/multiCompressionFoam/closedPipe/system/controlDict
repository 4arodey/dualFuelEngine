FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

/*
n = 92, RPM
EVO = 85, deg CA
deltaDeg = 1/6/n = 0.0018116, s
*/

application     multiCompressionFoam;//dyMFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.48; // (EVO + 180)/6/n

deltaT          1e-05;

writeControl    adjustableRunTime;

writeInterval   5e-03;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           0.1;

// maxDeltaT       1e-02;


