FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

injection       100;

inlet           50;

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

mInjection      #calc "-1.0*$injection";

mInlet          #calc "-1.0*$inlet";

boundaryField
{
/* Inlet */
    injection
    {
        type            uniformFixedValue;
        uniformValue    table
        (
            (0.25      (0           0 0))
            (0.26      ($mInjection 0 0))
            (0.36      ($mInjection 0 0))
            (0.37      (0           0 0))
        );
    }
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table
        (
            (0.072      (0       0 0))
            (0.075      ($mInlet 0 0))
            (0.215      ($mInlet 0 0))
            (0.216      (0       0 0))
        );
    }

/* Outlet */
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

/* Walls */
    "piston|valveHead"
    {
        type            movingWallVelocity;
        value           $internalField;
    }
    "valveStem|walls"
    {
        type            slip;
    }

/* ACMI & cyclic patches */
    #includeEtc "caseDicts/setConstraintTypes"

    "(inner|outer)ACMI(1|2)_blockage"
    {
        type            slip;
    }
    "(inner|outer)ACMI(1|2)_couple"
    {
        type            cyclicACMI;
        value           uniform (0 0 0);
    }
}

