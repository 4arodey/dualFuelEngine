FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitU
{
    type            limitVelocity;
    active          false;
    selectionMode   all;
    max             25;
}

limitT
{
    type            limitTemperature;
    active          false;
    selectionMode   all;
    min             200;
    max             2500;
}

