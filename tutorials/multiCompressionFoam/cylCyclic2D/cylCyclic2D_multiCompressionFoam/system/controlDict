FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     dyMFoam; // multiCompressionFoam \ dyMFoam

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.48;

deltaT          0.0018; // 1e-6 \ 0.0018

writeControl    adjustableRunTime;

writeInterval   $deltaT; // 0.00018 \ $deltaT

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  false; // true \ false

maxCo           0.1;


// ************************************************************************* //
