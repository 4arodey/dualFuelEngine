FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     moveMesh; // multiCompressionFoam \ moveMesh

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.48;

deltaT          0.0018; // 0.009 \ 0.0018

writeControl    adjustableRunTime;

writeInterval   0.0018;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  false; // true \ false

maxCo           1;


