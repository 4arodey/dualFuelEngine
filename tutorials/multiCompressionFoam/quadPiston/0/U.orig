FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

injection       50;

inlet           400;

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

minInjection    #calc "-1.0*$injection";

minInlet        #calc "-1.0*$inlet";

boundaryField
{
/* Injection */
    injectionXMinus
    {
        type            fixedValue;
        value           uniform ($minInjection 0 0);
    }
    injectionXPlus
    {
        type            fixedValue;
        value           uniform ($injection 0 0);
    }
    injectionYMinus
    {
        type            fixedValue;
        value           uniform (0 $minInjection 0);

    }
    injectionYPlus
    {
        type            fixedValue;
        value           uniform (0 $injection 0);
    }

/* Inlet */
    inletXMinus
    {
        type            fixedValue;
        value           uniform ($minInlet $inlet $inlet);
    }
    inletXPlus
    {
        type            fixedValue;
        value           uniform ($inlet $minInlet $inlet);
    }
    inletYMinus
    {
        type            fixedValue;
        value           uniform ($minInlet $minInlet $inlet);
    }
    inletYPlus
    {
        type            fixedValue;
        value           uniform ($inlet $inlet $inlet);
    }

/* Outlet */
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    
/* Walls */
    "injection(X|Y)(Minus|Plus)|piston|valve|walls"
    {
        type            slip;
    }

}

