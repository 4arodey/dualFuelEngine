FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0]; // K

internalField   uniform 850;

boundaryField
{
    // Injection
    "injection(X|Y)(Minus|Plus)"
    {
        type    fixedValue;
        value   $internalField; //uniform 400;
    }

    // Inlet
    "inlet(X|Y)(Minus|Plus)"
    {
        $injectionXMinus
    }

    // Outlet
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 764;
        value           $inletValue;
    }
    
    // Walls
    "piston|valve|walls"
    {
        type    zeroGradient;
    }
}

