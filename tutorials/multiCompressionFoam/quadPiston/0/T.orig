FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0]; // K

internalField   uniform 850;

boundaryField
{
/* Injection */
    "injection(X|Y)(Minus|Plus)"
    {
        type            fixedValue;
        value           uniform 380;
    }


/* Inlet */
    "inlet(X|Y)(Minus|Plus)"
    {
        type            fixedValue;
        value           uniform 332;
    }

/* Outlet */
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField; // uniform 769.3;
        value           $inletValue;
    }
    
/* Walls */
    "piston|valve|walls"
    {
        type    zeroGradient;
    }
}

// ************************************************************************* //
