FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

DT              DT [0 2 -1 0 0 0 0] 0.01; // m^2/s

MU              MU [1 -1 -1 0 0 0 0] 1e-02; // [kg/m/s = Pa·s], T = 20C˚
                                              // dinamic viscosity for air

GAMMA           GAMMA [0 0 0 0 0 0 0] 1.4; // heat capacity ratio

LAMBDA          LAMBDA [1 1 -3 -1 0 0 0] 2e-02; // [kg·m/s³/K = W/m/K]
                                               // coefficient of thermal
                                               // conductivity

// ************************************************************************* //
