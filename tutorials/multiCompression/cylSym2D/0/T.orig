FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [0 0 0 1 0 0 0]; // K

internalField           uniform 293.15; //1400;

boundaryField
{
    injection
    {
        type            fixedValue;
        value           uniform 293.15;
    }
    inlet
    {
        $injection
    }
    outlet
    {
        type            fixedValue;
        value           uniform 293.15;
    }
    piston
    {
        type            zeroGradient;
    }
    "(cyl|injection|inlet)_(front|back)"
    {   /* Sides */
        type            zeroGradient; // wedge;
    }
    "cyl_top|(injection|inlet)_topAndBottom"
    {   /* Other walls */
        type            zeroGradient;
    }
    "ACMI(1|2)_couple"
    {
        type            cyclicACMI;
        value           uniform 0;
    }
    "ACMI(1|2)_blockage"
    {
        type            zeroGradient;
    }
}

