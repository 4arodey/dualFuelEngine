FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiCompression;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5e-02;

deltaT          1e-05;

writeControl    timeStep; //runTime

writeInterval   5; // clockTime = writeInterval*deltaT

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  false;

maxCo           0.1;

maxDeltaT       1e-02;


