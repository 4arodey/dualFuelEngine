FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [0 0 0 1 0 0 0]; // K

internalField           uniform 293.15; //1400;

boundaryField
{
    // Injection
    "injectionXMinus|injectionXPlus|injectionYMinus|injectionYPlus"
    {
        type            codedFixedValue;
        value           uniform 293.15; //380;
        redirectType    T;
        code
        #{
            #include "../../constant/transportProperties.H"
            #include "../../../../../solvers/utilities/codedFixedValueBC/T.C"
        #};
    }

    // Inlet
    "inletXMinus|inletXPlus|inletYMinus|inletYPlus"
    {
        $injectionXMinus
    }

    // Outlet
    outlet
    {
        type            fixedValue;
        value           uniform 293.15;
    }
    
    // Walls
    "piston|valve|walls"
    {
        type            zeroGradient;
    }
}

