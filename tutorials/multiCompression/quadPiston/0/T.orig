FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      		[0 0 0 1 0 0 0]; // K

internalField   		uniform 289.4;

boundaryField
{
    // Injection
	"injectionXMinus|injectionXPlus|injectionYMinus|injectionYPlus"
    {
        type            uniformFixedValue;
        uniformValue    uniform 292.5;
    }

	// Inlet
	"inletXMinus|inletXPlus|inletYMinus|inletYPlus"
    {
        type            uniformFixedValue;
		uniformValue 	uniform 292.5;
    }

	// Other
    outlet
    {
        type            zeroGradient;
    } // outlet
    "piston|valve|walls"
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //