FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0]; // [Pa]

internalField   uniform 1e+05;

boundaryField
{
    // Injection
    "injection(X|Y)(Minus|Plus)"
    {
        type    fixedValue;
        value   uniform 2.5e+05;
        // type    codedFixedValue;
        // value   uniform 2.5e+05;
        // redirectType p;
        // code
        // #{
        //     #include "../../constant/transportProperties.H"
        //     #include "../../../../../solvers/utilities/codedFixedValueBC/p.C"
        // #};
    }
    
    // Inlet
    "inlet(X|Y)(Minus|Plus)"
    {
        $injectionXMinus
    }
    
    // Outlet
    outlet
    {
        type    zeroGradient;
    }
    
    // Walls
    "piston|valve|walls"
    {
        type    zeroGradient;
    }
}

