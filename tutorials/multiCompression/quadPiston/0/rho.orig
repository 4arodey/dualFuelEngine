FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions				[1 -3 0 0 0 0 0]; // kg/m^3

internalField   		uniform 1.787;

boundaryField
{
    // Injection
	"injectionXMinus|injectionXPlus|injectionYMinus|injectionYPlus"
    {
        type            zeroGradient;
    }
	
    // Inlet
	"inletXMinus|inletXPlus|inletYMinus|inletYPlus"
    {
        type            zeroGradient;
    }
	
	// Other
    outlet
    {
        type            fixedValue;
        value           uniform 1.204;
    }
    "valve|walls"
    {
        type            zeroGradient;
    }
}


