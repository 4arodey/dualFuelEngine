FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      		[0 1 -1 0 0 0 0]; // [kg m s K kg-mol cd]

internalField   		uniform (0 0 0);

injection 				150;

inlet 					75;

injMinus  				#calc "-1.0*$injection";

inletMinus				#calc "-1.0*$inlet";
	
boundaryField
{
    // Injection
	injectionXMinus
    {
		type            fixedValue;
		value           uniform ($injMinus 0 0);
    }
	injectionXPlus
    {
        type            fixedValue;
        value           uniform ($injection 0 0);
    }
	injectionYMinus
    {
        type            fixedValue;
        value           uniform (0 $injMinus 0);
    }
	injectionYPlus
    {
        type            fixedValue;
        value           uniform (0 $injection 0);
    }

	// Inlet
	inletXMinus
    {
        type            fixedValue;
        value           uniform ($inletMinus 0 $inlet);
	}
	inletXPlus
    {
        type            fixedValue;
        value           uniform ($inlet 0 $inlet);
    }
	inletYMinus
    {
        type            fixedValue;
        value           uniform (0 $inletMinus $inlet);
    }
	inletYPlus
    {
        type            fixedValue;
        value           uniform (0 $inlet $inlet);
    }

	// Outlet
    outlet
    {
		type            zeroGradient;
    }
	
	// Walls
    "piston|valve|walls"
    {
        type            slip;
    }
}

