FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    object      pointMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue
        {
            type        sine;
            frequency   1.4286; // n/60
            amplitude   3.13; // pi*S*n/60 = pi*0.65*92/60
            start       0.154; // EVO/6/n = -85/6/92
            level       0; // offset
        }
    }

    outlet
    {
        type            uniformFixedValue;
        uniformValue    constant 0;
    }

    walls
    {
        type            slip;
    }
}

// ************************************************************************* //