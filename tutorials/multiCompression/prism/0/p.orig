FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0]; // [kg/m/s^2 = Pa]

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 20e4; // 2 atm
    }
    outlet
    {

        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
}

