    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// General m4 macros
















// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;





 // relative to S & D

// ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ //








	




// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
	
vertices
(
	// Inner of piston
	/*00*/ (17.5 0 0) // Vertex cylIn0Down = 0 
	/*01*/ (0 -17.5 0) // Vertex cylIn1Down = 1 
	/*02*/ (-17.5 0 0) // Vertex cylIn2Down = 2 
	/*03*/ (0 17.5 0) // Vertex cylIn3Down = 3 
	
	// Outer of piston
	/*04*/ (35 0 0) // Vertex cylOut0Down = 4 
	/*05*/ (0 -35 0) // Vertex cylOut1Down = 5 
	/*06*/ (-35 0 0) // Vertex cylOut2Down = 6 
	/*07*/ (0 35 0) // Vertex cylOut3Down = 7 
	
	// Upper inner cylinder
	/*08*/ (17.5 0 280) // Vertex cylIn0Up = 8 
	/*09*/ (0 -17.5 280) // Vertex cylIn1Up = 9 
	/*10*/ (-17.5 0 280) // Vertex cylIn2Up = 10 
	/*11*/ (0 17.5 280) // Vertex cylIn3Up = 11 
	
	// Upper of cylinder
	/*12*/ (35 0 280) // Vertex cylOut0Up = 12 
	/*13*/ (0 -35 280) // Vertex cylOut1Up = 13 
	/*14*/ (-35 0 280) // Vertex cylOut2Up = 14 
	/*15*/ (0 35 280) // Vertex cylOut3Up = 15 
);

blocks
(
	// Inner cylinder block (8 9 10 11 0 1 2 3)
    hex (8 9 10 11 0 1 2 3) /*block 0*/
	(7 7 56)
    simpleGrading (1 1 1)
		
	// 1st quarter of cylinder (8 12 13 9 0 4 5 1)
    hex (8 12 13 9 0 4 5 1) /*block 1*/
    (7 7 56)
    simpleGrading (1 1 1)

	// 2nd quarter of cylinder (9 13 14 10 1 5 6 2)
	hex (9 13 14 10 1 5 6 2) /*block 2*/
	(7 7 56)
	simpleGrading (1 1 1)

	// 3rd quarter of cylinder (10 14 15 11 2 6 7 3)
    hex (10 14 15 11 2 6 7 3) /*block 3*/
    (7 7 56)
    simpleGrading (1 1 1)

    // 4th quarter of cylinder (11 15 12 8 3 7 4 0)
    hex (11 15 12 8 3 7 4 0) /*block 4*/
    (7 7 56)
    simpleGrading (1 1 1)
);

edges
(
	// Bottom of cylinder
	arc 4 5 (24.7487373637399 -24.7487373637399 0)
	arc 5 6 (-24.7487373637399 -24.7487373637399 0)
	arc 6 7 (-24.7487373637399 24.7487373637399 0)
	arc 7 4 (24.7487373637399 24.7487373637399 0)
		
	// Inner of piston (comment to make inner block as prism)
	arc 0 1 (12.3743686818699 -12.3743686818699 0)
	arc 1 2 (-12.3743686818699 -12.3743686818699 0)
	arc 2 3 (-12.3743686818699 12.3743686818699 0)
	arc 3 0 (12.3743686818699 12.3743686818699 0)
	
	// Upper of upper wall
	arc 12 13 (24.7487373637399 -24.7487373637399 280)
	arc 13 14 (-24.7487373637399 -24.7487373637399 280)
	arc 14 15 (-24.7487373637399 24.7487373637399 280)
	arc 15 12 (24.7487373637399 24.7487373637399 280)
		
	// Inner of upper wall (comment to make inner block as prism)
	arc 8 9 (12.3743686818699 -12.3743686818699 280)
	arc 9 10 (-12.3743686818699 -12.3743686818699 280)
	arc 10 11 (-12.3743686818699 12.3743686818699 280)
	arc 11 8 (12.3743686818699 12.3743686818699 280)
);

boundary
(
    walls
    {
        type wall;
        faces
        (
			(8 9 11 10)  // Upper inner
			(4 5 13 12) // 1st quarter of cylinder
			(5 6 14 13) // 2nd quarter of cylinder
			(6 7 15 14) // 3rd quarter of cylinder
			(7 4 12 15) // 4th quarter of cylinder
				
			(8 12 9 13) // 1st quarter of upper wall
			(9 13 10 14) // 2nd quarter of upper wall
			(10 14 11 15) // 3rd quarter of upper wall
			(11 15 8 12) // 4th quarter of upper wall
        );
    }
	
    piston
    {
        type wall;
        faces
        (
			(0 1 3 2) // Piston inner
				
			(0 4 1 5) // 1st quarter of piston
			(1 5 2 6) // 2nd quarter of piston
			(2 6 3 7) // 3rd quarter of piston
			(3 7 0 4) // 4th quarter of piston				
			
        );
    }
);

