FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      		[0 0 0 1 0 0 0]; // K

internalField   		uniform 289.4;

boundaryField
{
	inlet
    {
        type            uniformFixedValue;
		uniformValue 	uniform 292.5;
    }
	
    outlet
    {
		type            zeroGradient;
    }
	
	walls
    {
        type            slip;
    }
}

