FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions				[1 -3 0 0 0 0 0]; // kg/m^3

internalField   		uniform 1.204;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1.787;
    }
	
	outlet
    {
		type            zeroGradient;
    }
	
	walls
	{
        type            zeroGradient;
    }
}

