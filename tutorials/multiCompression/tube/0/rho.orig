FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphaAir;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [1 -3 0 0 0 0 0]; // kg/m^3

internalField           uniform 1.160; // 1e05/287.3/300 = 1.16022740457129597401

boundaryField
{
    inlet
    {
        type            codedFixedValue;
        value           uniform 1.508; // 13e04/287.3/300 = 1.50829562594268476621
        redirectType    rho;
        code
        #{
            #include "../../constant/transportProperties.H" /* get variables */
            #include "../../../../../solvers/utilities/codedFixedValueBC/rho.C"
            
            Info << "Calculated BC:  rho = " << p/R/T << endl;
        #};
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    walls
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
