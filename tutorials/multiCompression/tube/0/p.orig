FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0]; // [Pa]

internalField   uniform 1e+05; // 1e05/287.3/300 = 1.16022740457129597401

boundaryField
{
    inlet
    {
        type    fixedValue;
        value   uniform 1.3e+05;
        // type            codedFixedValue;
        // value           uniform 1.508; // 1.508*287.3*300 = 129974.52
        // redirectType    rho;
        // code
        // #{
        //     #include "../../constant/transportProperties.H" /* get variables */
        //     #include "../../../../../solvers/utilities/codedFixedValueBC/rho.C"
        // #};
    }

    outlet
    {
        type    zeroGradient;
    }

    walls
    {
        type    zeroGradient;
    }
}

