FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [0 0 0 1 0 0 0]; // K

internalField           uniform 293.15; //1400;

boundaryField
{

    // Inlet
    outlet
    {
        type            fixedValue;
        value           uniform 293.15;
    }

    // Outlet
    piston
    {
        type            fixedValue;
        value           uniform 293.15;
    }
    
    // Walls
    ".*"
    {
        type            zeroGradient;
    }
}

