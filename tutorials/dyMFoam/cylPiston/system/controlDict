FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application           dyMFoam;

startFrom             startTime;

startTime             0;

stopAt                endTime;

endTime               0.5;

deltaT                0.001;

writeControl          timeStep;

writeInterval         20;

purgeWrite            0;

writeFormat           ascii;

writePrecision        6;

writeCompression       off;

timeFormat            general;

timePrecision         6;

runTimeModifiable     true;


