{
    "alphaAir|alphaGas"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;
    }
	"rho|U"
    {
        solver          GAMG;
        smoother        DIC;
        tolerance       1e-06;
        relTol          0.01;
    }
	e
    {
        solver          GAMG;
        smoother        DILU;
        tolerance       1e-06;
        relTol          0.01;
    }
    cellMotionUy
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0;
    }
