FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    object      pointMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    // "inlet|channel*|ACMI1*"
    "(inlet|outlet|channelFront|channelBack|channelTopAndBottom|ACMI1_couple|ACMI1_blockage)"
    {
        type            uniformFixedValue;
        uniformValue    constant 0;
    }

    "(piston|cylFront|cylBack|axis|ACMI2_couple|ACMI2_blockage)"
    {
        type            slip;
    }

    piston
    {
        type            uniformFixedValue;
        uniformValue    constant 5;
    }
}

