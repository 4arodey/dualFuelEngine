FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [1 -3 0 0 0 0 0]; // kg/m^3

internalField           uniform 1.160; // 1.160*287.3*293.15 = 97697.5142

boundaryField
{
    // Inlet
    outlet
    {
        type            fixedValue;
        value           uniform 1.508; // 25e04/287.3/380 = 2.28992250902229468554
    }

    // Outlet
    piston
    {
        type            zeroGradient;
    }

    // Walls
    ".*"
    {
        type            zeroGradient;
    }
}

