FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application          dyMFoam;

startFrom            startTime;

startTime            0;

stopAt               endTime;

endTime              0.48;

deltaT               0.0018;

writeControl         adjustableRunTime;

writeInterval        $deltaT;

purgeWrite           0;

writeFormat          ascii;

writePrecision       6;

writeCompression     off;

timeFormat           general;

timePrecision        6;

runTimeModifiable    true;

adjustTimeStep       false;

maxCo                0.3;

