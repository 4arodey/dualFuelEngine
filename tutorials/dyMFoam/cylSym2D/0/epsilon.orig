FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1.8e-3;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

/* Inlet & Injection */
    injection
    {
        type            fixedValue;
        value           $internalField;
    }
    inlet
    {
        $injection
    }

/* Outlet */
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

/* Walls */
    piston
    {
        type            zeroGradient;
    }
    "cyl_top|(injection|inlet)_(back|front|topAndBottom)"
    {
        type            zeroGradient;
    }

/* ACMI patches */
    "ACMI(1|2)_blockage"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    "ACMI(1|2)_couple"
    {
        type            cyclicACMI;
        value           $internalField;
    }
}


