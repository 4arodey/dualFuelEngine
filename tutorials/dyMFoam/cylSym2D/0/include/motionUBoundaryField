boundaryField
{
    piston
    {
        type            uniformFixedValue;
        uniformValue    // FIXME: sine has differrent members for of7x & of-dev
        { 
            type        sine; // of-dev
            frequency   1.4286; // n/60
            amplitude   13; // pi*S*n/60 = pi*2.7*92/60
            start       0; // EVO/6/n = -85/6/92
            level       0; // offset
        }
    }

    // Patches //
    "(injection|inlet|(injection|inlet)_(topAndBottom|front|back)|outlet|cyl_top)"
    {
        type            uniformFixedValue;
        uniformValue    constant 0;
    }
    "axis|cyl_(front|back)"
    {
        type            slip;
    }

    // Fixed ACMIs //
    ACMI1_couple
    {
        type            uniformFixedValue;
        patchType       cyclicACMI;
        uniformValue    0;
    }
    ACMI1_blockage
    {
        type            uniformFixedValue;
        uniformValue    0;
    }

    // Moving ACMIs //
    ACMI2_couple
    {
        type            slip;
        patchType       cyclicACMI;
        value           uniform 0;
    }
    ACMI2_blockage
    {
        type            slip;
        value           uniform 0;
    }
}
