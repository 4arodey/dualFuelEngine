FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0]; // m^2/s^2 = Pa/(kg/m^3)

internalField   uniform 0;

boundaryField
{
    // Injection
	injectionXMinus
    {
        type            zeroGradient;
    }
    injectionXPlus
    {
        type            zeroGradient;
    }
    injectionYMinus
    {
        type            zeroGradient;
    }
    injectionYPlus
    {
        type            zeroGradient;
    }
	
    // Inlet
	inletXMinus
    {
        type            zeroGradient;
    }
    inletXPlus
    {
        type            zeroGradient;
    }
    inletYMinus
    {
        type            zeroGradient;
    }
    inletYPlus
    {
        type            zeroGradient;
    }
	
	// Other
    outlet
    {
        type            fixedValue;
        value           uniform 0; 
    }
    valve
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
}

